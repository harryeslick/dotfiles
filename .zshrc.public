
# fix error  command not found: compdef
autoload -U compinit
compinit

alias config="/usr/bin/git --git-dir=$HOME/.cfg/ --work-tree=$HOME"

export EDITOR=nano

vizsh () {
  vim ~/.zshrc
  . ~/.zshrc
}

vizshl () {
  vim ~/.zshrc.local
  . ~/.zshrc
}


eval "$(gh completion -s zsh)"

source ~/.zsh_aliases
[ -f ~/.zsh_secrets ] && . ~/.zsh_secrets
